{
  "name": "nexprint",
  "version": "0.1.0",
  "private": true,
  "scripts": {
    "dev": "concurrently \"npm run watch:icons\" \"next dev --turbopack\"",
    "watch:icons": "chokidar 'src/shared/components/Icon/svg/*.svg' -c 'npm run generate:icons'",
    "build": "npm run generate:icons && next build --turbopack",
    "start": "next start",
    "generate:icons": "tsx src/shared/components/Icon/configs/generate-icons.ts",
    "lint": "eslint"
  },
  "dependencies": {
    "@floating-ui/react": "^0.27.16",
    "@iconify/react": "^6.0.1",
    "bcryptjs": "^3.0.2",
    "class-variance-authority": "^0.7.1",
    "clsx": "^2.1.1",
    "dayjs": "^1.11.18",
    "framer-motion": "^12.23.14",
    "next": "15.5.3",
    "react": "19.1.0",
    "react-dom": "19.1.0",
    "tailwind-merge": "^3.3.1"
  },
  "devDependencies": {
    "@eslint/eslintrc": "^3",
    "@iconify/utils": "^3.0.1",
    "@tailwindcss/postcss": "^4",
    "@types/css-modules": "^1.0.5",
    "@types/fs-extra": "^11.0.4",
    "@types/node": "^20",
    "@types/react": "^19.1.13",
    "@types/react-dom": "^19.1.9",
    "chokidar-cli": "^3.0.0",
    "concurrently": "^9.2.1",
    "eslint": "^9",
    "eslint-config-next": "15.5.3",
    "eslint-config-prettier": "^10.1.8",
    "eslint-plugin-prettier": "^5.5.4",
    "fs-extra": "^11.3.1",
    "prettier": "^3.6.2",
    "tailwindcss": "^4",
    "ts-node": "^10.9.2",
    "tsx": "^4.20.5",
    "typescript": "^5.9.2"
  }
}
